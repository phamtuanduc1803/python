score=input('Enter score : ')
try:
    s=float(score)
    if s>=0.9 && s<=1.0:
        print('A')
    elif s>=0.8 && s<0.9:
        print('B')
    elif s>=0.7 && s<0.8:
        print('C')
    elif s>=0.6 && s<0.7:
        print('D')
    elif s<0.6:
        print('F')
    else:
        print('Bad score')
except:
    print('Bad score')
        
